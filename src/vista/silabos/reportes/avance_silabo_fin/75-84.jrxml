	</parameter>
	<queryString>
		<![CDATA[select count(ac.id_almn_curso)
	from  
	"AlumnoCurso" ac join "Cursos" c ON c.id_curso=ac.id_curso 
	join "Alumnos"  a on ac.id_alumno=a.id_alumno join "Personas" p on p.id_persona=a.id_persona
	where  C.id_curso=$P!{id_curso} and  ROUND(ac.almn_curso_nota_final,1)>=75 and ROUND(ac.almn_curso_nota_final,1)<85 and ac.almn_curso_activo='true' and  ac.almn_curso_asistencia='Asiste']]>
