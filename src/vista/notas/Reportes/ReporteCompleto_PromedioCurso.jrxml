	<queryString>
		<![CDATA[SELECT
ROUND(Avg("public"."AlumnoCurso".almn_curso_nota_final),0) AS "PROMEDIO CURSO"
FROM
"public"."AlumnoCurso"
INNER JOIN "public"."Cursos" ON "public"."AlumnoCurso".id_curso = "public"."Cursos".id_curso AND ''= ''
INNER JOIN "public"."PeriodoLectivo" ON "public"."Cursos".id_prd_lectivo = "public"."PeriodoLectivo".id_prd_lectivo
INNER JOIN "public"."Jornadas" ON "public"."Cursos".id_jornada = "public"."Jornadas".id_jornada
WHERE
"public"."Cursos".id_docente = $P{id_docente} AND
"public"."PeriodoLectivo".prd_lectivo_nombre = $P{prd_lectivo_nombre} AND
"public"."Cursos".curso_ciclo = $P{curso_ciclo} AND
"public"."Cursos".curso_paralelo = $P{curso_paralelo} AND
"Jornadas".nombre_jornada = $P{nombre_jornada}]]>
	</queryString>
