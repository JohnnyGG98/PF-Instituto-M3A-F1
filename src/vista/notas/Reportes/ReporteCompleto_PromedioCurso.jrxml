<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ReporteCompleto_PromedioCurso" language="groovy" pageWidth="250" pageHeight="555" orientation="Landscape" columnWidth="250" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT
ROUND(Avg("public"."AlumnoCurso".almn_curso_nota_final),0) AS "PROMEDIO CURSO"
FROM
"public"."AlumnoCurso"
INNER JOIN "public"."Cursos" ON "public"."AlumnoCurso".id_curso = "public"."Cursos".id_curso AND ''= ''
INNER JOIN "public"."PeriodoLectivo" ON "public"."Cursos".id_prd_lectivo = "public"."PeriodoLectivo".id_prd_lectivo
INNER JOIN "public"."Jornadas" ON "public"."Cursos".id_jornada = "public"."Jornadas".id_jornada
WHERE
"public"."PeriodoLectivo".prd_lectivo_estado IS TRUE AND
"public"."Cursos".id_docente = 55 AND
"public"."PeriodoLectivo".prd_lectivo_nombre = 'SDS   MAYO/2019   OCTUBRE/2019' AND
"public"."Cursos".curso_ciclo = 4 AND
"public"."Cursos".curso_paralelo = 'A' AND
"Jornadas".nombre_jornada = 'MATUTINA']]>
	</queryString>
	<field name="PROMEDIO CURSO" class="java.math.BigDecimal"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<detail>
		<band height="22" splitType="Stretch">
			<textField>
				<reportElement mode="Opaque" x="115" y="2" width="100" height="20" forecolor="#000000" backcolor="#FFFF99"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{PROMEDIO CURSO}]]></textFieldExpression>
			</textField>
			<rectangle>
				<reportElement mode="Transparent" x="115" y="2" width="100" height="20"/>
			</rectangle>
			<rectangle>
				<reportElement mode="Transparent" x="15" y="2" width="100" height="20"/>
			</rectangle>
			<staticText>
				<reportElement mode="Opaque" x="15" y="2" width="100" height="20" forecolor="#FFFFFF" backcolor="#0033CC"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[PROMEDIO CURSO]]></text>
			</staticText>
		</band>
	</detail>
</jasperReport>
