JOIN public."PeriodoLectivo" p USING(id_prd_lectivo))
JOIN public."Docentes" d USING(id_docente)
JOIN public. "Personas" per USING (id_persona)
WHERE p.prd_lectivo_nombre =  $P{periodolectivo}   
