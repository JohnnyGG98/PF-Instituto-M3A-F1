               p.id_persona = a.id_persona AND
              pr.id_prd_lectivo = c.id_prd_lectivo AND
               c.curso_ciclo = $P{ciclo} AND
               pr.prd_lectivo_nombre = $P{periodo} AND
               ac.id_curso = c.id_curso 
AND almn_curso_activo=true
