	<queryString>
		<![CDATA[SELECT
"ViewAlumnoCurso".id_alumno,
"ViewAlumnoCurso".p_alu.persona_identificacion,
"ViewAlumnoCurso".p_alu.persona_primer_apellido || ' '
||"ViewAlumnoCurso".p_alu.persona_segundo_apellido as "APELLIDOS",
"ViewAlumnoCurso".p_alu.persona_primer_nombre || ' ' || " ViewAlumnoCurso".p_alu.persona_segundo_nombre AS "NOMBRES",
ROUND("ViewAlumnoCurso".almn_curso_nt_1_parcial, 1) AS "APORTE 1",
ROUND("ViewAlumnoCurso".almn_curso_nt_examen_interciclo, 1) AS "INTERCICLO",
ROUND("ViewAlumnoCurso".almn_curso_nt_2_parcial, 1) AS "APORTE 2",
ROUND("ViewAlumnoCurso".almn_curso_nt_examen_final, 1) AS "EXAMEN FINAL",
ROUND("ViewAlumnoCurso".almn_curso_nt_examen_supletorio, 1) AS "SUPLETORIO",
"ViewAlumnoCurso".almn_curso_asistencia,
ROUND("ViewAlumnoCurso".almn_curso_nota_final, 0) AS "NOTA FINAL",
"ViewAlumnoCurso".almn_curso_estado,
"ViewAlumnoCurso".almn_curso_num_faltas,
"ViewAlumnoCurso".materia_nombre,
"ViewAlumnoCurso".nombre_jornada,
"ViewAlumnoCurso".materia_ciclo,
"ViewAlumnoCurso".curso_paralelo,
"ViewAlumnoCurso".carrera_nombre,
"ViewAlumnoCurso".prd_lectivo_nombre,
"ViewAlumnoCurso".persona_primer_nombre || ' ' ||"ViewAlumnoCurso".persona_segundo_nombre as "NOMBRE_PROF",
"ViewAlumnoCurso".persona_primer_apellido || ' ' ||"ViewAlumnoCurso".persona_segundo_apellido as "APELLIDO_PROF",
("ViewAlumnoCurso".almn_curso_num_faltas * "ViewAlumnoCurso".materia_total_horas)/100 as "% Faltas",
"ViewAlumnoCurso".persona_primer_apellido || ' ' || "ViewAlumnoCurso".persona_segundo_apellido as "APELLIDOS_COORDINADOR",
"ViewAlumnoCurso".persona_primer_nombre || ' ' ||"ViewAlumnoCurso".persona_segundo_nombre AS "NOMBRES_COORDINADOR"
FROM
"ViewAlumnoCurso"
WHERE
"ViewAlumnoCurso".id_docente = $P{id_docente}
AND "ViewAlumnoCurso".prd_lectivo_nombre = $P{prd_lectivo_nombre}
AND "ViewAlumnoCurso".curso_ciclo = $P{curso_ciclo}
AND "ViewAlumnoCurso".curso_paralelo = $P{curso_paralelo}
AND "ViewAlumnoCurso".nombre_jornada = $P{nombre_jornada}
AND "ViewAlumnoCurso".almn_curso_nota_final < 70
ORDER BY
"ViewAlumnoCurso".persona_primer_apellido ASC]]>
	</queryString>
