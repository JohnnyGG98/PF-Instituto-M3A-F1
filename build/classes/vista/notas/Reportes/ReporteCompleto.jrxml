	<queryString>
		<![CDATA[SELECT
	"ViewAlumnoCurso".id_alumno,
	"ViewAlumnoCurso".persona_identificacion,
	"ViewAlumnoCurso".persona_primer_apellido || ' ' || "ViewAlumnoCurso".persona_segundo_apellido AS "APELLIDOS",
	"ViewAlumnoCurso".persona_primer_nombre || ' ' || "ViewAlumnoCurso".persona_segundo_nombre AS "NOMBRES",
	ROUND( "ViewAlumnoCurso".almn_curso_nt_1_parcial, 1 ) AS "APORTE 1",
	ROUND( "ViewAlumnoCurso".almn_curso_nt_examen_interciclo, 1 ) AS "INTERCICLO",
	ROUND( "ViewAlumnoCurso".almn_curso_nt_2_parcial, 1 ) AS "APORTE 2",
	ROUND( "ViewAlumnoCurso".almn_curso_nt_examen_final, 1 ) AS "EXAMEN FINAL",
	ROUND( "ViewAlumnoCurso".almn_curso_nt_examen_supletorio, 1 ) AS "SUPLETORIO",
	"ViewAlumnoCurso".almn_curso_asistencia,
	ROUND( "ViewAlumnoCurso".almn_curso_nota_final, 0 ) AS "NOTA FINAL",
	"ViewAlumnoCurso".almn_curso_estado,
	"ViewAlumnoCurso".almn_curso_num_faltas,
	"ViewAlumnoCurso".materia_nombre,
	"ViewAlumnoCurso".nombre_jornada,
	"ViewAlumnoCurso".curso_ciclo,
	"ViewAlumnoCurso".curso_paralelo,
	"ViewAlumnoCurso".carrera_nombre,
	"ViewAlumnoCurso".prd_lectivo_nombre,
	"ViewAlumnoCurso".pri_nom_docente || ' ' || "ViewAlumnoCurso".seg_nom_docente AS "NOMBRE_PROF",
	"ViewAlumnoCurso".pri_ape_docente || ' ' || "ViewAlumnoCurso".seg_ape_docente AS "APELLIDO_PROF",
	"ViewAlumnoCurso".coor_prim_apellido || ' ' || "ViewAlumnoCurso".coor_segn_apellido AS "APELLIDOS_COORDINADOR",
	"ViewAlumnoCurso".coor_prim_nombre || ' ' || "ViewAlumnoCurso".coor_segn_nombre AS "NOMBRES_COORDINADOR" 
FROM
	"ViewAlumnoCurso" 
WHERE
	"public"."ViewAlumnoCurso".id_docente = $P{id_docente} 
	AND "public"."ViewAlumnoCurso".prd_lectivo_nombre = $P{prd_lectivo_nombre}
	AND "public"."ViewAlumnoCurso".curso_ciclo =  $P{curso_ciclo}
	AND "public"."ViewAlumnoCurso".curso_paralelo =  $P{curso_paralelo}
	AND "public"."ViewAlumnoCurso".nombre_jornada = $P{nombre_jornada}
	AND "public"."ViewAlumnoCurso".materia_nombre = $P{materia_nombre}
ORDER BY "ViewAlumnoCurso".persona_primer_apellido ASC]]>
	</queryString>
